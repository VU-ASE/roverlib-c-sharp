{
  "name": "ASE-roverlib-csharp",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".",
    "args": {
      "HOST_UID": "1000"
    }
  },
  "mounts": [
    "source=${localWorkspaceFolder},target=/workspace/roverlib-c-sharp,type=bind"
  ],
  "workspaceFolder": "/workspace/roverlib-c-sharp",
  "customizations": {
    "vscode": {
      "extensions": [
        "streetsidesoftware.code-spell-checker",
        "ms-dotnettools.csharp",
        "ms-dotnettools.dotnet-format",
        "editorconfig.editorconfig",
        "dbankier.vscode-quick-select"
      ]
    }
  },
  "remoteUser": "dev"
}
